{
  "name": "mypage",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "node server/app.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint .",
    "slint": "stylelint \"src/**/*.(vue|scss|css)\"",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@ngify/http": "^1.5.3",
    "@vueuse/components": "^10.4.1",
    "@vueuse/core": "^10.4.1",
    "@vueuse/integrations": "^10.4.1",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "element-plus": "^2.3.12",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "ngify": "^1.6.2",
    "rxjs": "^7.8.1",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/express": "^4.17.17",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.5.9",
    "@vercel/node": "^3.0.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.48.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "sass": "^1.66.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix",
    "*.(vue|scss|css)": "stylelint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu"
    ],
    "rules": {
      "no-console": "off"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      "*.scss",
      "*.css",
      "*.webp",
      "server/app.js"
    ]
  }
}
